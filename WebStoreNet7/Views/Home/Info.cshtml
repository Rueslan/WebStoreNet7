@model WebStoreNet7.Models.Employee

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Инфо</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>#</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th>Подробно</th>
            </tr>
        </thead>
        <tbody>
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.LastName</td>
                    <td>@Model.FirstName</td>
                    <td>@Model.Patronymic</td>
                    <td>@Model.Age</td>
                    <td>@Model.Info</td>
                </tr>
        </tbody>
    </table>
    <p>
        <a href="/Home/Employees">Назад</a>
    </p>
</body>
</html>
